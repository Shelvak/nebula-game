         xmlns:galaxy="components.map.space.galaxy.*"
         minWidth="0" minHeight="0">
   <fx:Script>
      <![CDATA[
      import models.solarsystem.MSSMetadata;
      import models.solarsystem.SSMetadataType;


      [Bindable]
      public var model: MSSMetadata;
      ]]>
   </fx:Script>
   <s:layout>
      <s:VerticalLayout horizontalAlign="justify" verticalAlign="top"/>
   </s:layout>
   <galaxy:CSingleOwnerAssets model="{new CSingleOwnerAssetsM(model.playerMetadata)}"/>
   <galaxy:CSingleOwnerAssets model="{new CSingleOwnerAssetsM(model.alliesMetadata)}"/>
   <galaxy:CSingleOwnerAssets model="{new CSingleOwnerAssetsM(model.napsMetadata)}"/>
